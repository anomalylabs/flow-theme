        "popper.js": "^1.12",
        "mousetrap": "^1.6.3",
        "noty": "^3.2.0-beta",
        "postcss": "^7.0.23",
        "sass": "^1.23.3",
        "sass-loader": "^7.3.1",
        "tailwindcss": "^1.1.2",
        "tailwind-css-variables": "^2.0.3",
        "tailwindcss-transitions": "^2.1.0",
        "vue": "^2.6.10",
        "vue-template-compiler": "^2.6.10",
        "@fortawesome/fontawesome-free": "^5.11.2"
